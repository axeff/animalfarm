<script>

  $(document).ready(function() {
    $( window ).resize(function() {
        $('.about_content').css('margin-top', $('picture').height()+'px');
        $('.about_content img').css('top', '-'+$('picture').height()+'px');
    });
    $('.about_content').css('margin-top', $('picture').height()+'px');
    $('.about_content img').css('top', '-'+$('picture').height()+'px');

    $(window).scroll(function(){
      var amount = 57 + (Math.floor($(window).scrollTop()/2));
      $('.about_wrapper > picture').css('top', amount+'px');
    });
    $('.navigation').stickUp({
      parts: ['about','news','ads','lineup','programm','newsletter','infos'],
      itemClass: 'navigation__item',
      itemHover: 'active',
    });
    if ($(window.location.hash+'').length) {
      //take id with name from hash param and scroll there
      $(window.location.hash+'').animatescroll();
    }

    $('.navigation a.navigation__item').each(function(i, el){
      $(el).click(function(){
        event.preventDefault();
        $('#'+$(el).data('target')+'').animatescroll({
          onScrollEnd: function(){
            location.hash="#"+$(el).data('target');
          }
        });
      });
    })

    if ($(document).scrollTop() <= 1) {
      $('.navigation').removeClass('isStuck');
    }

    $(document).on('scroll', function() {
      if ($(document).scrollTop() <= 1) {
        $('.navigation').removeClass('isStuck');
      }
    });

  });
</script>
